{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "env": {
          "ANDROID_NATIVE_DEBUG_SYMBOLS": "false"
        }
      },
      "ios": {}
    },
    "production-arm64": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "env": {
          "BUILD_ARM64_ONLY": "true"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}